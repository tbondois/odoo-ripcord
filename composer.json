{
    "name": "tbondois/odoo-ripcord",
    "description": "Ripoo : a PHP8 XML-RPC client handler for Odoo External API",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "phpxmlrpc/polyfill-xmlrpc": "*@rc",
        "darkaonline/ripcord": "^0.3"
    },
    "require-dev": {
        "kint-php/kint": "*"
    },
    "autoload": {
        "psr-4": {
            "Ripoo\\": "src/"
        }
    },
    "license": "MIT",
    "readme": "README.md",
    "homepage": "https://packagist.org/packages/tbondois/odoo-ripcord",
    "authors": [
        {   "name": "Thomas Bondois"
        },
        {
            "name": "Matt Visnovsky",
            "email": "matt@visnovsky.us",
            "homepage": "https://www.visnovsky.us"
        }
    ],
    "keywords": [
        "Odoo",
        "API",
        "RPC",
        "XML",
        "xmlrpc",
        "ripcord",
        "Magento2",
        "XML-RPC"
    ]
